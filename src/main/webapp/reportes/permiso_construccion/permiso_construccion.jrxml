	<queryString>
		<![CDATA[select substring('00'||terreno.cod_provincia FROM (character_length('00'||terreno.cod_provincia)-1)) ||--lpad(cast(predio.cod_provincia as varchar),2, '0')--
substring('00'||terreno.cod_canton FROM (character_length('00'||terreno.cod_canton)-1)) ||
substring('00'||terreno.cod_parroquia FROM (character_length('00'||terreno.cod_parroquia)-1)) ||
substring('00'||terreno.cod_zona FROM (character_length('00'||terreno.cod_zona)-1)) ||
substring('00'||terreno.cod_sector FROM (character_length('00'||terreno.cod_sector)-1)) ||
substring('000'||terreno.cod_manzana FROM (character_length('000'||terreno.cod_manzana)-2))  ||
substring('000'||terreno.cod_solar FROM (character_length('000'||terreno.cod_solar)-2))   AS codigo_predial, predio.id as id_predio, * FROM flow.tr_permiso_construccion pc
INNER JOIN flow.ht_tramite tramite ON pc.tramite = tramite.id
LEFT OUTER JOIN cat_predio predio ON tramite.predio = predio.id
LEFT OUTER JOIN cat_terreno terreno ON predio.cod_parroquia = terreno.cod_parroquia AND
predio.cod_zona = terreno.cod_zona AND
predio.cod_sector = terreno.cod_sector AND
predio.cod_manzana = terreno.cod_manzana AND
predio.cod_solar = terreno.cod_solar
LEFT OUTER JOIN sector as sector ON sector.cod_sector = terreno.cod_sector
AND sector.cod_zona = terreno.cod_zona AND sector.cod_parroquia = terreno.cod_parroquia
LEFT OUTER JOIN parroquia as parroquia ON parroquia.cod_parroquia = terreno.cod_parroquia
AND tramite.id = $P{ID_TRAMITE} ]]>
	</queryString>
